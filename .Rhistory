icils_23_proc <- icils_23_proc %>% rename("sexo"= S_SEX,
"aprendizaje_escuela"= S_LRNINTS,
"alf_digital"= PV1CIL,
"autoeffgen"= S_GENCLASS,
"iiseb"= IISB,
"inmigrante"= S_IMMBGR,
"educ_p1"= IS3G09,
"educ_p2"= IS3G13,
"libros_hogar"= IS3G14)
icils_23_proc <- icils_23_proc %>%
mutate(
