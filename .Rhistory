"admin_dig" = egov_egov)
View(qog_23)
pib_df$pib <- pib_df$pib * 0.001 # la reconvertimos para que su unidad sean miles de dolares
pib_df$logpib <- log(pib_df$pib)
icils23_completa <- merge(icils_23_proc, pib_df, by = "CNTRY", all.x = TRUE)
